f = open("sample.txt","r")
data = f.read()
print(data)
f.close()  # tis is used to read the file

f = open("another,txt","w")
f.write("this is a king")  # this is used to write the file
f.close()

f = open("another.txt","a")
f.write("this is a boy") # this is used to apend the text end of the write
f.close() # text

with open("another.txt","r") as f:
    a = f.read()
with open("another.txt","w") as f:
    a = f.write("this is ram")
print(a) 
